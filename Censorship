client.on('message', message=>{

    const args = message.content.slice().split(/ +/);
    const messageCensor = args.shift().toLowerCase();;

    

    if(messageCensor === 'urban'){
      
         message.channel.send('You mean a fucking bitch');
    } else if(messageCensor === 'canada'){
        message.channel.send('What a fucking Boomer')
    } else if(messageCensor === 'demise'){
        message.channel.send('Who?')
    } else if(messageCensor === "Can I join the clan?"){
        message.channel.send('For what game?')
    } else if(messageCensor === 'fortnite'){
        message.channel.send('Message Hades')
    } else if(messageCensor === 'plague'){
        message.channel.send('No <3')
    } else if(messageCensor === 'cynical'){
        message.channel.send('Fuck off')
    }
    
})

//put in main.js